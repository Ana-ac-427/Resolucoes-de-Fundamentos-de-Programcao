using System;

class Program
{
    static void Main()
    {
        // Leitura dos valores
        int A = int.Parse(Console.ReadLine());
        int B = int.Parse(Console.ReadLine());

        // Cálculo da soma
        int X = A + B;

        // Impressão do resultado
        Console.WriteLine($"X = {X}");
    }
}

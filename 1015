-- Função para calcular a distância entre dois pontos
function calcular_distancia(x1, y1, x2, y2)
    return math.sqrt((x2 - x1)^2 + (y2 - y1)^2)
end

-- Leitura dos valores
local x1, y1 = io.read("*n", "*n")  -- Lê os valores de x1 e y1
local x2, y2 = io.read("*n", "*n")  -- Lê os valores de x2 e y2

-- Cálculo da distância
local distancia = calcular_distancia(x1, y1, x2, y2)

-- Impressão da saída formatada com 4 casas decimais
print(string.format("%.4f", distancia))
